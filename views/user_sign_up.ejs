<h1 align="center">Sign Up</h1>
<link rel="stylesheet" href="<%= assetPath("/css/signup.css") %>">
<form action="/users/create" method="post" id="signup">
    <input type="text" name="name" placeholder="Your Name" required class="textbox">
    <input type="email" name="email" placeholder="Your Email" required class="textbox">
    <input type="password" name="password" placeholder="Password" required class="textbox">
    <input type="password" name="confirm_password" placeholder="Confirm Password" required class="textbox">
    <p>
        <a href="/users/auth/google">Google Sign In</a>
    </p>
    <input type="submit" value="Sign Up" id="signupbutton">
</form>